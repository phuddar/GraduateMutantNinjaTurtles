CREATE TABLE Referee(
   Referee_ID int NOT NULL AUTO_INCREMENT,
   fname varchar(255),
   lname varchar(255) NOT NULL,
   age int,
   Referee_grade (int),
   Assignment_status varchar(255),
   Referee_skill_rating int,
   PRIMARY KEY(Referee_ID,lname)
);

CREATE TABLE Game(
   Game_ID int NOT NULL AUTO_INCREMENT,
   Game_Time time,
   Game_Date date,
   Game_Field varchar(255),
   Referee_1 int,
   Referee_2 int,
   Referee_3 int,
   Referee_4 int
   PRIMARY KEY(Game_ID),
   FOREIGN KEY(Referee_1) REFERENCES Referee(Referee_ID),
   FOREIGN KEY(Referee_2) REFERENCES Referee(Referee_ID),
   FOREIGN KEY(Referee_3) REFERENCES Referee(Referee_ID),
   FOREIGN KEY(Referee_4) REFERENCES Referee(Referee_ID)
);

